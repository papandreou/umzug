{
  "presets": [
    ["env", {
      "targets": {
        "node": 6
      }
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "dynamic-import-node",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true,
      "helpers": false
    }]
  ]
}
